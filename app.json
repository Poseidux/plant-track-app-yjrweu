
{
  "expo": {
    "name": "Verdant Tracker",
    "slug": "Verdant Tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/80ca3ca5-8576-4ab3-90e8-216642077eac.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/80ca3ca5-8576-4ab3-90e8-216642077eac.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a7f64"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.Natively",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "icon": "./assets/images/80ca3ca5-8576-4ab3-90e8-216642077eac.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/d9af1230-f55f-4ddc-818c-21c5109452a7.png",
        "backgroundColor": "#1a7f64"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.Natively",
      "icon": "./assets/images/d9af1230-f55f-4ddc-818c-21c5109452a7.png"
    },
    "web": {
      "favicon": "./assets/images/80ca3ca5-8576-4ab3-90e8-216642077eac.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "scheme": "natively",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {}
    }
  },
  "scheme": "Verdant Tracker"
}
