
{
  "expo": {
    "name": "Tree Planting Tracker",
    "slug": "Natively",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/final_quest_240x240.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/final_quest_240x240.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a7f64"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.Natively",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "splash": {
        "image": "./assets/images/final_quest_240x240.png",
        "resizeMode": "contain",
        "backgroundColor": "#1a7f64"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/final_quest_240x240.png",
        "backgroundColor": "#1a7f64"
      },
      "splash": {
        "image": "./assets/images/final_quest_240x240.png",
        "resizeMode": "contain",
        "backgroundColor": "#1a7f64",
        "mdpi": "./assets/images/final_quest_240x240.png",
        "hdpi": "./assets/images/final_quest_240x240.png",
        "xhdpi": "./assets/images/final_quest_240x240.png",
        "xxhdpi": "./assets/images/final_quest_240x240.png",
        "xxxhdpi": "./assets/images/final_quest_240x240.png"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.Natively"
    },
    "web": {
      "favicon": "./assets/images/final_quest_240x240.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/final_quest_240x240.png",
          "imageWidth": 240,
          "resizeMode": "contain",
          "backgroundColor": "#1a7f64"
        }
      ]
    ],
    "scheme": "natively",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {}
    }
  }
}
