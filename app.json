
{
  "expo": {
    "name": "Verdant Tracker",
    "slug": "Verdant Tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/f2037505-6eb5-442d-a722-42f1b9412ebb.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/f2037505-6eb5-442d-a722-42f1b9412ebb.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a7f64"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.Natively",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/f2037505-6eb5-442d-a722-42f1b9412ebb.png",
        "backgroundColor": "#1a7f64"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.Natively"
    },
    "web": {
      "favicon": "./assets/images/f2037505-6eb5-442d-a722-42f1b9412ebb.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "scheme": "natively",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {}
    }
  },
  "scheme": "Verdant Tracker"
}
